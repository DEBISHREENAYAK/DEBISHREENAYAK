
a=2
b=3

c=a>b and b<a #and
print(c)

d=not a and not b #or
print(d)

e=not(a>4)
print(e)

f=a<4 and b>6
print(f)



#you can also get input at runtime
#eg:

a=int(input("enter a number "))
b=int(input("enter a number "))


a=2
b=3

c=a>b and b<a #and
print(c)

d=not a and not b #or
print(d)

e=not(a>4)
print(e)

f=a<4 and b>6
print(f)



